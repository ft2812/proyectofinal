funcion reorganizacion(vec,sel,riv)
	dimension vaux[8]
	definir n como entero 
	//primero en el vector nuevo cargo todos los nros que sean distintos a sel y a riv del vec original
	//despues los elimina y carga de nuevo al vector orginal
	n=1
	para i<-1 hasta 8 con paso 1
		
		si vec[i]<>sel y vec[i]<>riv
			
			vaux[n]<-vec[i]
			///mostrar vaux[n] 
			n<-n+1
		FinSi
		
	FinPara
	para x<-1 hasta 8 con paso 1
		vec[x]<-vaux[x]
		///mostrar "*vec siendo modificado *",vec[x]
	FinPara
