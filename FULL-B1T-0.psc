Funcion riv<-busqueda(sel,az)///busca al nro de equipo seleccionado por el jugador, al rival y los elimina del vec de equipos
	para j<-1 hasta 8 con paso 1
	si sel==az[j]
		si j mod 2 == 0 Entonces
			riv<-az(j-1)
		SiNo
			riv<-az(j+1)
		FinSi
	FinSi
