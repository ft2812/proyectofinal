Funcion resto(vec,bnro,bnom,control)///recordar variable control
	///va a modificar el sector de 4 en caso de los cambios de orden necesarios
	dimension vaux[6]
	definir largo,i,equi1,equi2,g,n,entradas,aux como entero
	n<-1
	largo<-longi(vec)//llama a longi para ver cuantos equipos hay cargados si 6 o 4
	
	segun largo
		
		6:mostrar"RESULTADOS ACTUALES:"
			
			para i<-1 hasta 5 con paso 2 Hacer
				//mostrar "valor de i: ",i
				equi1<-vec[i]
				equi2<-vec[i+1]
				
				g<-simulacion(equi1,equi2,bnro,bnom)
				vaux[n]<-g
				n<-n+1
			FinPara
			
			
			
		4:mostrar"entro en 4 y c vale: ",control
			para i<-1 hasta 3 con paso 2
				equi1<-vec[i]
				equi2<-vec[i+1]
				
				g<-simulacion(equi1,equi2,bnro,bnom)
				
				si g==equi1 entonces
					p<-equi2
				sino 
					p<-equi1
				FinSi
				vaux[i]<-g
				vaux[i+1]<-p
			fin para 
			
		2:mostrar "entraron 2 equipos"
			equi1<-vec[1]
			equi2<-vec[2]
			
			g<-simulacion(equi1,equi2,bnro,bnom)
			
			si g==equi1 entonces
				p<-equi2
			sino 
				p<-equi1
			FinSi
			vaux[1]<-g
			vaux[2]<-p
			
	FinSegun
	///este control tiene que ser bien pensado a fin de no generar confusiones
	///y en caso de ultima instancia el vazar debe ser pasado a vecfin*************************************************
	si (largo==4 y control==1)
		aux<-vaux[3]
		vaux[3]<-vaux[2]
		vaux[2]<-aux
	FinSi
	
	para x<-1 hasta 6 con paso 1 Hacer
		///mostrar "vector queda en" ,vaux[x]
		vec[x]<-vaux[x]///se asigna los datos del vector interno auxiliar al vazar que es el principal
	FinPara
	
Finfuncion 

Funcion gan<-simulacion(eq1,eq2,bnro,bnom)///recibe 2 equipos, llama a funcion favorito, y con eso simula goles mostrando el
	///resultado final de cada encuentro,devuelve un ganador
	definir g1,g2 como entero
	fav<-favorito(eq1,eq2,bnro)
	
	si fav==eq1 entonces
		g1<-azar(5)+2
		g2<-azar(2)
		gan<-eq1
	SiNo
		g1<-azar(2)
		g2<-azar(5)+2
		gan<-eq2
	FinSi
	
	mostrar"RESULTADO DEL ENCUENTRO: ",bnom[12,eq1]," | ",g1,"| vs |",g2," | ",bnom[12,eq2]
	
FinFuncion


